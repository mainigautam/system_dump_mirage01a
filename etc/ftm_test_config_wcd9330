tc 1
#Handset Capture
!Capture
Txdevice:0

enable
MultiMedia1 Mixer TERT_MI2S_TX:1
DEC1 MUX:ADC1

disable
DEC1 MUX:ZERO
MultiMedia1 Mixer TERT_MI2S_TX:0

tc 2
#Ear Piece
!Playback
Rxdevice:0

enable
